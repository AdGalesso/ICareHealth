<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>ICareHealth - The best 4 you</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/fontawesome")
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.5/angular.min.js"></script>
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="container body-content container-fluid">
        <a href="https://github.com/AdGalesso/ICareHealth" target="_blank">
            <img class="fork-me" src="https://s3.amazonaws.com/github/ribbons/forkme_right_green_007200.png" alt="Fork me on GitHub">
        </a>
        @RenderBody()
    </div>
    <footer>
        <p>&copy; @DateTime.Now.Year - The best plan for you!</p>
    </footer>
    @RenderSection("scripts", required: false)
</body>
</html>
